{% extends 'base.html.twig' %}

{% block title %}{{ function.name }}{% endblock %}

{% block body %}
    <div class="dashboard-container p-5">
        <h1 class="text-center p-5">{{ function.name }}</h1>
        <p class="text fs-4 fw-bold">{{ function.definition }}</p>
        <p class="text fs-5"><pre class="function-example bg-dark text-white p-2"><code>{{ formattedExample }}</code></pre></p>
    </div>
{% endblock %}
